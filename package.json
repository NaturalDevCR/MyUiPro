{
  "name": "my-ui-pro",
  "version": "2.1.0",
  "description": "Soundcraft Ui Multiframe Wrapper",
  "productName": "My Ui Pro",
  "author": "Josue Orozco A. <35145882+NaturalDevCR@users.noreply.github.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:win64-binaries": "quasar build -m electron -T win32 -A x64",
    "build:darwin64-binaries": "quasar build -m electron -T darwin -A x64",
    "build:all-x64-binaries": "quasar build -m electron -T all -A x64",
    "build:all-arm-binaries": "quasar build -m electron -T all",
    "build:mac-universal": "quasar build -m electron -T darwin -A universal",
    "build:win64": "quasar build -m electron -T win32 -A x64",
    "build:spa": "quasar clean && quasar build -m spa --single-file",
    "build:bex": "quasar clean && quasar build -m bex"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@quasar/extras": "^1.16.12",
    "@vueuse/core": "^11.2.0",
    "events": "^3.3.0",
    "is-ip": "^5.0.1",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^3.2.3",
    "quasar": "^2.17.1",
    "soundcraft-ui-connection": "^3.0.0",
    "vue": "^3.5.12",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^10.0.6",
    "vue-router": "^4.4.5",
    "vue3-slider": "^1.10.1",
    "webmidi": "^3.1.11"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@quasar/app-vite": "^2.0.0-beta.24",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.2",
    "electron": "^32.2.0",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.6.3",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-singlefile": "^2.0.3",
    "vue-tsc": "2"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
