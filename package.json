{
  "name": "my-ui-pro-next",
  "version": "2.1.1",
  "description": "Multiframe Wrapper for Soundcraft Ui Mixers",
  "productName": "My Ui Pro",
  "author": "Josue <35145882+NaturalDevCR@users.noreply.github.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "build:win64-binaries": "quasar build -m electron -T win32 -A x64",
    "build:darwin64-binaries": "quasar build -m electron -T darwin -A x64",
    "build:all-x64-binaries": "quasar build -m electron -T all -A x64",
    "build:all-arm-binaries": "quasar build -m electron -T all",
    "build:mac-universal": "quasar build -m electron -T darwin -A universal",
    "build:win64": "quasar build -m electron -T win32 -A x64",
    "build:spa": "quasar clean && quasar build -m spa --single-file",
    "build:bex": "quasar clean && quasar build -m bex -T chrome"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "@vueuse/core": "^13.3.0",
    "events": "^3.3.0",
    "is-ip": "^5.0.1",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "quasar": "^2.18.1",
    "soundcraft-ui-connection": "^3.2.0",
    "vite-plugin-singlefile": "^2.2.0",
    "vue": "^3.5.16",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1",
    "vue3-slider": "^1.10.1",
    "webmidi": "^3.1.12"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "electron": "^36.4.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
