  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "create-win64-binaries": "quasar build -m electron -T win32 -A x64",
    "create-darwin64-binaries": "quasar build -m electron -T darwin -A x64",
    "create-all-x64-binaries": "quasar build -m electron -T all -A x64",
    "create-all-arm-binaries": "quasar build -m electron -T all",
    "build-mac-universal": "quasar build -m electron -T darwin -A universal",
    "build-win64": "quasar build -m electron -T win32 -A x64",
    "build-spa": "quasar clean && quasar build -m spa --single-file",
    "build-bex": "quasar clean && quasar build -m bex --node-pollyfills",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
